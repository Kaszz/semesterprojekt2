<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Presentation.EditUsersController">
   <children>
      <fx:include source="BasicFramework.fxml" />
      <Label layoutX="67.0" layoutY="114.0" text="Fornavn" />
      <Label layoutX="67.0" layoutY="149.0" text="Efternavn" />
      <Label layoutX="67.0" layoutY="187.0" text="Email" />
      <Label layoutX="67.0" layoutY="220.0" text="Password" />
      <TextField fx:id="userFirstNameTextField" layoutX="172.0" layoutY="109.0" onAction="#userFirstNameEntered" />
      <TextField fx:id="userLastNameTextField" layoutX="172.0" layoutY="144.0" onAction="#userLastNameEntered" />
      <TextField fx:id="userEmailTextField" layoutX="172.0" layoutY="180.0" onAction="#userEmailEntered" />
      <TextField fx:id="userPasswordTextField" layoutX="172.0" layoutY="217.0" onAction="#userPasswordEntered" />
      <Button fx:id="deleteUserButton" layoutX="162.0" layoutY="287.0" mnemonicParsing="false" onAction="#deleteUserButtonClicked" text="Slet" />
      <Button fx:id="changeUserButton" layoutX="220.0" layoutY="287.0" mnemonicParsing="false" onAction="#changeUserButtonClicked" text="Tilføj" />
      <Button fx:id="createUserButton" layoutX="283.0" layoutY="287.0" mnemonicParsing="false" onAction="#createUserButtonClicked" text="Opret" />
      <Label layoutX="67.0" layoutY="20.0" text="Administrér brugere" AnchorPane.leftAnchor="70.0">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TreeTableView fx:id="usersTreeTableView" layoutX="355.0" layoutY="71.0" prefHeight="313.0" prefWidth="231.0">
        <columns>
          <TreeTableColumn fx:id="nameUserTreeTableView" prefWidth="75.0" text="Navn" />
          <TreeTableColumn fx:id="emailUserTreeTableView" prefWidth="75.0" text="Email" />
            <TreeTableColumn fx:id="passwordUserTreeTableView" text="Password" />
        </columns>
      </TreeTableView>
   </children>
</AnchorPane>
