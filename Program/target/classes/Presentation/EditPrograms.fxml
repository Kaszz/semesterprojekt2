<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Presentation.EditProgramsController">
   <children>
      <fx:include source="BasicFramework.fxml" />
      <VBox layoutX="28.0" layoutY="116.0" prefHeight="291.0" prefWidth="100.0" AnchorPane.leftAnchor="28.0">
         <children>
            <Label minHeight="32.0" text="Titel" />
            <Label minHeight="32.0" text="Type" />
            <Label minHeight="32.0" text="Trailer URL" />
            <Label minHeight="32.0" text="Beskrivelse" />
            <Label minHeight="32.0" text="Lancering ">
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin></Label>
            <Label minHeight="32.0" text="Episode titel" visible="false" />
            <Label minHeight="32.0" text="Sæson" visible="false" />
            <Label minHeight="32.0" prefHeight="33.0" text="Episode nr" visible="false" />
         </children>
      </VBox>
      <VBox layoutX="155.0" layoutY="117.0" prefHeight="306.0" prefWidth="224.0">
         <children>
            <TextField fx:id="titleTextField" minHeight="24.0" onAction="#titleTextFieldOnAction">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </TextField>
            <ComboBox fx:id="broadcastTypeComboBox" minHeight="24.0" prefHeight="27.0" prefWidth="223.0">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </ComboBox>
            <TextField fx:id="trailerURLTextField" accessibleText="hhtp://" minHeight="24.0" onAction="#trailerURLTextFieldOnAction" promptText="http::/">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="descriptionTextField" minHeight="24.0" onAction="#descriptionTextFieldOnAction">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </TextField>
            <DatePicker fx:id="launchDatePicker" onAction="#launchDatePickerOnAction" prefHeight="27.0" prefWidth="223.0">
               <VBox.margin>
                  <Insets bottom="35.0" />
               </VBox.margin>
            </DatePicker>
            <TextField fx:id="episodeTextField" onAction="#episodeTextFieldOnAction" prefHeight="24.0" visible="false">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </TextField>
            <ComboBox fx:id="seasonComboBox" minHeight="24.0" onAction="#seasonComboBoxOnAction" prefHeight="27.0" prefWidth="222.0" visible="false">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </ComboBox>
            <ComboBox fx:id="episodeNoComboBox" onAction="#episodeNoComboBoxOnAction" prefHeight="27.0" prefWidth="222.0" visible="false">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </ComboBox>
         </children>
      </VBox>
      <Label layoutX="368.0" layoutY="23.0" text="Administrér programmer" AnchorPane.leftAnchor="368.0">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TreeTableView fx:id="programTreeTableView" layoutX="410.0" layoutY="78.0" prefHeight="485.0" prefWidth="455.0">
        <columns>
          <TreeTableColumn fx:id="titleColumn" prefWidth="203.0" text="Title" />
          <TreeTableColumn fx:id="subTitleColumn" prefWidth="201.0" text="Subtitle" />
            <TreeTableColumn fx:id="yearMadeColumn" prefWidth="50.0" text="År" />
        </columns>
      </TreeTableView>
      <Button layoutX="334.0" layoutY="464.0" mnemonicParsing="false" text="Gem" />
      <Button layoutX="280.0" layoutY="464.0" mnemonicParsing="false" text="Slet" />
